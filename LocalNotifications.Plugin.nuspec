<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
   <metadata minClientVersion="2.8.1">
       <id>Xam.Plugin.LocalNotifications</id>
       <version>2.0.0.0</version>
       <title>Local Notifications Plugin for Xamarin and Windows</title>
       <authors>Saveliy Bondini</authors>
       <owners>Saveliy Bondini</owners>
     <licenseUrl>https://github.com/B1naryStudio/Xamarin.LocalNotifications/blob/master/LICENSE.md</licenseUrl>
     <projectUrl>https://github.com/B1naryStudio/Xamarin.LocalNotifications</projectUrl>
       <requireLicenseAcceptance>false</requireLicenseAcceptance>
       <description>
         Send Local Notifications across iOS, Android, and Windows UWP from a single API. 
         
         Ability to create, send and cancel local notifications.
       </description>
       <summary>Create and parameterize local notifications, schedule them or send immediately, cancel if required</summary>
       <tags>xamarin, local notification, android, uwp, ios</tags>
       <dependencies>
		  <group targetFramework="netstandard1.0">
			<dependency id="NETStandard.Library" version="1.0.0" />
		  </group>
		  <group targetFramework="MonoAndroid10">
		  </group>
		  <group targetFramework="Xamarin.iOS10">
		  </group>
		  <group targetFramework="uap">
		  </group>
		</dependencies>
   </metadata>
   <files>
	 <!--Core-->
     <file src="LocalNotifications\LocalNotifications.Plugin\bin\Release\netstandard1.0\" target="lib\netstandard1.0" />

     <!--UWP-->
     <file src="LocalNotifications\LocalNotifications.Plugin.UWP\bin\Release\" target="lib\UAP10" />

     <!--Xamarin.Android-->
     <file src="LocalNotifications\LocalNotifications.Plugin.Android\bin\Release\" target="lib\MonoAndroid10" />
     
     <!--Xamarin.iOS Unified-->
     <file src="LocalNotifications\LocalNotifications.Plugin.iOSUnified\bin\iPhone\Release\" target="lib\Xamarin.iOS10" />
   </files>
</package>